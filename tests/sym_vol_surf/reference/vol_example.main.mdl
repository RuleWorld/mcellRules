rxn_layer_t =  0.01
vol_wall =  56.5695045056029  /*Surface area*/
vol_EC =  39
vol_PM =  4.836624601  /*Surface area*/
vol_CP =  1
NaV =  602200000
Lig_tot =  6.0e3
Rec_tot =  4.0e2
kp1 =  1
km1 =  0.01
kp2 =  1
km2 =  0.01
