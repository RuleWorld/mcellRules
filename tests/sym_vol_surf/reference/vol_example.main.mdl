Rc =  0.0015      /* Radius of a (cylindrical) molecule in 2D compartment, um */
gamma =  0.5722      /* Euler's constant */
KB =  1.3806488e-19     /* Boltzmann constant, cm^2.kg/K.s^2 */
mu_wall =  1e-9      /* Viscosity in compartment wall, kg/um.s */
